{
    "name": "Token Exchange",
    "api_id": "token-exchange",
    "org_id": "default",
    "definition": {
        "location": "header",
        "key": "version"
    },
    "use_keyless": false,
    "version_data": {
        "not_versioned": true,
        "versions": {
            "Default": {
                "name": "Default",
                "expires": "3000-01-02 15:04",
                "use_extended_paths": true,
                "extended_paths": {
                    "ignored": [],
                    "white_list": [],
                    "black_list": []
                }
            }
        }
    },
    "proxy": {
        "listen_path": "/token-exchange/",
        "target_url": "http://httpbin.org",
        "strip_listen_path": true
    },
    "token_exchange_options": {
        "enable": true
    }
}